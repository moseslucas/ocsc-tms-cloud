%td= "#{o.branch[0][0..2].upcase}-#{o.ref_id}"
%td= o.client.name
%td= o.trans_date
%td= o.branch[0].titleize
%td= o.destination.name
%td
  - if o.status1 == 1
    %span.label.label-warning
      OPEN
  - elsif o.status1 == 2
    %span.label.label-default
      CLOSED
%td= o.total_amount
-# %td= o.payments.sum(:amount)
-# %td= (o.total_amount - (o.payments.sum(:amount)))
%td
  = button_to "View", payment_path(o), method: "get", class: "btn dark btn-outline"
